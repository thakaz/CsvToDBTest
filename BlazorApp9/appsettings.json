{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": ""
  },

  "CsvFolderPath": "C:\\path\\csvfolder",

  "FileModelMappings": [
    {
      "FileName": "Department",
      "ModelType": "BlazorApp9.Models.Department",
      "MapModelType": "BlazorApp9.Models.CsvMap.DepartmentMap",
      "UpdateMethod": "replace"
    },
    {
      "FileName": "Employee",
      "ModelType": "BlazorApp9.Models.Employee",
      "MapModelType": "BlazorApp9.Models.CsvMap.EmployeeMap",
      "UpdateMethod": "add"
    }
  ]
}
